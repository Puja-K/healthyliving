<% allcoursesbyinstructor.each do |c| %>
                        <div class="courseDetails courseDetailsSideBar">
                            <%= image_tag c.image.url(:thumb), class: "yoga", alt: "yoga"%>
                            
                        </div>
                        <h5 class="pl8 pr8"><%= c.name %></h5>
                        <div class="pl8 pr8 courseDesc"><%= c.description %></div>
                        <div class="courseInfo bgF6 font11">
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="favDiv pl8 pt8">
                                        <i class="fa fa-heart colorc52666"></i> Add to Favorite
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="classificationDiv pt8">
                                        <%=c.level%>
                                    </div>
                                </div>
                            </div>
                            <div class="row">

                                <div class="col-lg-6">
                                    <div class="ratingDiv pt5 pb5">
                                         <div class ="star-rating" data-score="<%=@avg_rating%>"></div>
                                        <span class="font10">  
                                            <% if c.reviews %>
                                                <%= "#{c.reviews.length} reviews" %>
                                            <%else%>
                                                No Reviews
                                            <%end%>
                                        </span>
                                    </div>
                                   
                                </div>
                                <div class="col-lg-6">&nbsp; </div>
                            </div>
                        </div>
                        <%end %>
                        <%= will_paginate allcoursesbyinstructor, renderer: BootstrapPagination::Rails %>